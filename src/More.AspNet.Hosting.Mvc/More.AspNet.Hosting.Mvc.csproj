 </ItemGroup>
 -->

 <ItemGroup>
  <Reference Include="System.Web" />
 </ItemGroup>

 <ItemGroup>
  <PackageReference Include="Microsoft.AspNet.Mvc" Version="5.2.3" />
 </ItemGroup>

 <ItemGroup>
  <ProjectReference Include="..\More.AspNet.Hosting\More.AspNet.Hosting.csproj" />
 </ItemGroup>

 <Import Project="..\Shared\Shared.projitems" Label="Shared" />

</Project>